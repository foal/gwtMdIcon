<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.jresearch.commons.gwt.mdIcon</groupId>
		<artifactId>org.jresearch.commons.gwt.mdIcon.pom</artifactId>
		<version>1.0.0-SNAPSHOT</version>
		<relativePath>../pom/pom.xml</relativePath>
	</parent>
	<artifactId>org.jresearch.commons.gwt.mdIcon.icon</artifactId>
	<name>JRS GWT https://materialdesignicons.com content</name>
	<packaging>gwt-lib</packaging>
	<dependencies>
		<dependency>
			<groupId>org.jresearch.dominokit</groupId>
			<artifactId>domino-ui</artifactId>
		</dependency>
		<dependency>
			<groupId>org.jresearch.commons.gwt.mdIcon</groupId>
			<artifactId>org.jresearch.commons.gwt.mdIcon.processor</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.google.code.findbugs</groupId>
			<artifactId>jsr305</artifactId>
		</dependency>
		<dependency>
			<groupId>com.google.guava</groupId>
			<artifactId>guava</artifactId>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<annotationProcessorPaths>
						<path>
							<groupId>org.jresearch.commons.gwt.mdIcon</groupId>
							<artifactId>org.jresearch.commons.gwt.mdIcon.processor</artifactId>
							<version>${project.version}}</version>
						</path>
					</annotationProcessorPaths>
				</configuration>
			</plugin>
			<plugin>
				<groupId>net.ltgt.gwt.maven</groupId>
				<artifactId>gwt-maven-plugin</artifactId>
				<configuration>
					<moduleName>org.jresearch.commons.gwt.mdIcon</moduleName>
					<moduleShortName>icon</moduleShortName>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
